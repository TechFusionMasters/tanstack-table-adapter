{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "noUnusedLocals": false,
    "noUnusedParameters": false
  },
  "typedocOptions": {
    "entryPoints": ["src/index.ts"],
    "out": "storybook-static/api",
    "name": "TanStack Table Adapter API",
    "includeVersion": true,
    "excludePrivate": true,
    "excludeExternals": true,
    "hideGenerator": true,
    "readme": "README.md",
    "plugin": ["typedoc-plugin-markdown"]
  }
}